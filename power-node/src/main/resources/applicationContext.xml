<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--
        注册bean对象：
            id:自定义对象的名称,通过id在代码中使用对象
            class:类的全限定名称,不能是接口
    -->
    <bean id="someServiceImpl" class="cn.clown.service.impl.SomeServiceImpl"/>
    <bean id="Date" class="java.util.Date"/>

    <bean id="student" class="cn.clown.domain.Student">
        <constructor-arg name="name" value="张三"/>
        <constructor-arg name="age" value="23"/>
        <constructor-arg name="school" ref="school"/>
    </bean>

    <bean id="date" class="java.util.Date">
        <property name="time" value="2021"/>
    </bean>

    <bean id="school" class="cn.clown.domain.School">
        <property name="name" value="power-node"/>
        <property name="address" value="beijing"/>
    </bean>


    <bean id="student1" class="cn.clown.domain.Student">
        <property name="name" value="Mr.zhan"/>
        <property name="age" value="20"/>
        <property name="school" ref="school"/>
    </bean>

    <bean id="file" class="java.io.File">
        <constructor-arg name="parent" value="E:\idea_space\Learning-spring\power-node\src\main\resources"/>
        <constructor-arg name="child" value="applicationContext.xml"/>
    </bean>

    <bean id="myStudent" class="cn.clown.domain.Student" autowire="byName">
        <property name="name" value="张三"/>
        <property name="age" value="23"/>
    </bean>

    <bean id="byTypeStudent" class="cn.clown.domain.Student" autowire="byType">
        <property name="name" value="张三1"/>
        <property name="age" value="231"/>
    </bean>

    <import resource="spring-school.xml"/>
    <import resource="spring-student.xml"/>
</beans>